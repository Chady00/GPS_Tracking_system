#include "tm4c123gh6pm.h"
#include "stdint.h"
#include <math.h>

#ifndef M_PI
#define M_PI 3.141592 // defining the value of PI 
double getdistance(double lat_1,double long_1,double lat_2,double long_2)
    {
    double R = 6371.0 ; // radius value in km
    double distance = 0 ; // initializing distance value.
    double d_lat = (lat_2 - lat_1)*M_PI/180.0; 
    double d_long = (long_2 - long_1)*M_PI/180.0 ;}
    
